{
  "name":        "dreamfactory/df-rackspace",
  "description": "Rackspace package containing Rackspace/OpenStack components for the DreamFactory(tm) core.",
  "keywords":    [
    "dreamfactory",
    "api",
    "rest",
    "rackspace",
    "openstack",
    "storage blob"
  ],
  "homepage":    "https://www.dreamfactory.com/",
  "license":     "Apache-2.0",
  "authors":     [
    {
      "name":  "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    },
    {
      "name":  "Lee Hicks",
      "email": "leehicks@dreamfactory.com"
    }
  ],
  "support":     {
    "email":  "dspsupport@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/df-rackspace",
    "issues": "https://github.com/dreamfactorysoftware/df-rackspace/issues",
    "wiki":   "https://wiki.dreamfactory.com"
  },
  "minimum-stability": "dev",
  "prefer-stable":     true,
  "require":     {
    "dreamfactory/df-file":    "~0.7",
    "rackspace/php-opencloud": "^1.16"
  },
  "autoload":    {
    "psr-4": {
      "DreamFactory\\Core\\Rackspace\\": "src/"
    }
  },
  "extra":       {
    "branch-alias": {
      "dev-develop": "0.14.x-dev"
    },
    "laravel": {
      "providers": [
        "DreamFactory\\Core\\Rackspace\\ServiceProvider"
      ]
    }
  }
}

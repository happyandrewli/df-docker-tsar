    "dreamfactory",
    "dsp",
    "rest",
    "iot",
    "amqp",
    "rabbitmq",
    "mqtt",
    "pubsub"
  ],
  "homepage":    "https://github.com/dreamfactorysoftware/df-pubsub",
  "license":     "proprietary",
  "authors":     [
    {
      "name":  "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    }
  ],
  "support":     {
    "email":  "dspsupport@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/df-pubsub",
    "issues": "https://github.com/dreamfactorysoftware/df-pubsub/issues",
    "wiki":   "https://wiki.dreamfactory.com"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require":     {
    "dreamfactory/df-core": "~0.21"
  },
